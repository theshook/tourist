<div class="row text-center">
  <% rows.forEach(function(row){ %>
  <div class="container py-3">
    <div class="card">
      <div class="row ">
        <div class="col-md-4">
          <img src="uploads/<%= row.image_filename %>" class="w-100">
        </div>
        <div class="col-md-8 px-3">
          <div class="card-body">
            <h3 class="text-left">
              <%= row.spot_name %>
            </h3>
            <p class="card-text text-left text-truncate">
              <%= row.spot_description %>
            </p>
            <a href="/<%= route %>/<%= row.spot_no %>" class="btn btn-primary float-left my-3">View more</a>
          </div>
        </div>
      </div>
    </div>
  </div>
  <% }); %>
</div>